            <column name="os_name" type="VARCHAR(255)"/>
            <column name="srcs_version" type="VARCHAR(255)"/>
            <column name="master_src_mac" type="VARCHAR(255)"/>
            <column name="src_mac" type="VARCHAR(255)"/>
            <column name="srcserver" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>