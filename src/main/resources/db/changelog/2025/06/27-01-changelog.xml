            <column name="tz" type="VARCHAR(255)"/>
            <column name="log_id" type="VARCHAR(255)"/>
            <column name="type" type="VARCHAR(255)"/>
            <column name="sub_type" type="VARCHAR(255)"/>
            <column name="level" type="VARCHAR(255)"/>
            <column name="vd" type="VARCHAR(255)"/>
            <column name="srcip" type="VARCHAR(255)"/>
            <column name="src_name" type="VARCHAR(255)"/>
            <column name="src_port" type="BIGINT"/>
            <column name="src_intf" type="VARCHAR(255)"/>
            <column name="src_intf_role" type="VARCHAR(255)"/>
            <column name="dstip" type="VARCHAR(255)"/>
            <column name="dst_port" type="BIGINT"/>
            <column name="dst_intf" type="VARCHAR(255)"/>
            <column name="dst_intf_role" type="VARCHAR(255)"/>
            <column name="src_country" type="VARCHAR(255)"/>
